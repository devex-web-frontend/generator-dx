{
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "jsx": "react",
    "strictNullChecks": true,
    "lib": [
      "es2016",
      "dom",
      "es2017.object"
    ],
    "outDir": "dist",
    "target": "es5",
    "pretty": true
  }
}
